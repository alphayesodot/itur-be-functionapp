{
  "bindings": [
    {
      "name": "psiphasResultsBlob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "psiphas/{name}.xml",
      "connection": "AzureWebJobsStorage"
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "fileuploadqueue",
      "queueName": "%FILE_UPLOAD_QUEUE_NAME%",
      "connection": "AzureWebJobsStorage"
    }
  ],
  "scriptFile": "../dist/psiphas-results-handler/index.js"
}
