{
  "bindings": [
    {
      "name": "psiphasResultsBlob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "psiphas/{name}.xml",
      "connection": "AzureWebJobsStorage"
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "malshabqueue",
      "queueName": "malshab-queue",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "PdfBlob",
      "type": "blob",
      "path": "%PDF_BLOB_NAME%/psiphas-{DateTime}.pdf",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ],
  "scriptFile": "../dist/psiphas-results-handler/index.js"
}
