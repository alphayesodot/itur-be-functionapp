    },
    {
      "type": "queue",
      "direction": "out",
      "name": "eventsqueue",
      "queueName": "events-queue",
      "connection": "AzureWebJobsStorage"
    }
  ],
  "scriptFile": "../dist/itur-ramot-handler/index.js"
}
