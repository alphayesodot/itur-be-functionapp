{
  "bindings": [
    {
      "name": "xmlBlob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "itur-ramot/{name}.xml",
      "connection": "AzureWebJobsStorage"
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "malshabqueue",
