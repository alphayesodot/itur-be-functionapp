{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "HamanBlob",
      "type": "blob",
      "path": "%HAMAN_BLOB_NAME%/Haman-{DateTime}.xml",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    },
    {
      "name": "IturRamotBlob",
      "type": "blob",
      "path": "%ITUR_RAMOT_BLOB_NAME%/IturRamot-{DateTime}.xml",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    },
    {
      "name": "PsiphasBlob",
      "type": "blob",
      "path": "%PSIPHAS_BLOB_NAME%/Psiphas-{DateTime}.xml",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ],
  "scriptFile": "../dist/xml-upload/index.js"
}